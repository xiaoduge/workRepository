/*******************************************************************************
* Function Name  : Refresh_Page
* Description    : None
* Input          : None
* Output         : None
* Return         : None
* Attention      : None
*******************************************************************************/
void Refresh_Page(void)
{
  Page_1();
  Page_2();
  Page_2_1();
  Page_2_2();
  Page_2_3();
  Page_2_4();
  Page_2_5();
  Page_2_6();
  Page_2_7();  

  if ((RefreshDisplayPageNo != 0) && (Start_Flag == 1))
  {
    if (PageNo != 22)
    {
      Operate_PCF8563(PCF8563_Read, PCF8563_Register_Hours, 0);//Read Hours
      Value_Hour = ((RAM_PCF8563 >> 4) & 0x03) * 10 + (RAM_PCF8563 & 0x0F);
      Operate_PCF8563(PCF8563_Read, PCF8563_Register_Minutes, 0);//Read Minutes
      Value_Minute = ((RAM_PCF8563 >> 4) & 0x07) * 10 + (RAM_PCF8563 & 0x0F);
      Operate_PCF8563(PCF8563_Read, PCF8563_Register_VL_Seconds, 0);//Read Seconds
      Value_Second = ((RAM_PCF8563 >> 4) & 0x07) * 10 + (RAM_PCF8563 & 0x0F);
      Operate_PCF8563(PCF8563_Read, PCF8563_Register_Years, 0);//Read Year
      Value_Year = ((RAM_PCF8563 >> 4) & 0x07) * 10 + (RAM_PCF8563 & 0x0F);
      Operate_PCF8563(PCF8563_Read, PCF8563_Register_Century_Months, 0);//Read Month
      Value_Month = ((RAM_PCF8563 >> 4) & 0x01) * 10 + (RAM_PCF8563 & 0x0F);
      Operate_PCF8563(PCF8563_Read, PCF8563_Register_Days, 0);//Read Day
      Value_Day = ((RAM_PCF8563 >> 4) & 0x03) * 10 + (RAM_PCF8563 & 0x0F);
      Operate_PCF8563(PCF8563_Read, PCF8563_Register_Weekdays, 0);//Read Weekdays
      Value_Weekday = RAM_PCF8563 & 0x07;
    }

    if ((DI1 == 0) && (Mod == 0))
    {
      Mod = 1;
    }
    else
      if ((DI1 != 0) && (Mod == 1))
      {
        Mod = 0;
        PJST_Timer = 0;
      }

    if (DI2 == 0)   //DI2==1DI2==0
    {
      Counter_A = 0;
      Counter_B = 0;

      DO1_Reset;
      LED1_R_Off;
      LED1_G_Off;
      DO2_Reset;
      LED2_R_Off;
      LED2_G_Off;
    }
    else
      if ((Mod == 0) && (PageNo != 24))
      {
        Run_Flag = 0;
        Rest_Flag = 0;

        for (Holiday_No = 1; Holiday_No <= 70; Holiday_No++)
        {
          if ((Holiday_Data[Holiday_No - 1][Holiday_Month] >> 4 & 0x01) != 0)
          {
            if ((Holiday_Data[Holiday_No - 1][Holiday_Year]) == Value_Year)
            {
              if ((Holiday_Data[Holiday_No - 1][Holiday_Month] & 0x0F) == Value_Month)
              {
                if ((Holiday_Data[Holiday_No - 1][Holiday_Day] & 0x0F) == Value_Day)
                {
                  Rest_Flag = 1;
                }
              }
            }
          }
        }

        if ((((Weekday_Data[Value_Weekday][Weekday_Hour_Start] >> 7) & 0x01) != 0) && (Rest_Flag == 0))
        {
          if ((((Weekday_Data[Value_Weekday][Weekday_Hour_Start] & 0x7F) * 60 + Weekday_Data[Value_Weekday][Weekday_Minute_Start]) <= (Value_Hour * 60 + Value_Minute)) &&
              ((Weekday_Data[Value_Weekday][Weekday_Hour_Stop] * 60 + Weekday_Data[Value_Weekday][Weekday_Minute_Stop]) >= (Value_Hour * 60 + Value_Minute)))
          {
            Run_Flag = 1;
          }
          else
          {
            Rest_Flag = 1;
          }
        }
        else
        {
          Rest_Flag = 1;
        }

        if ((Run_Flag != 0) && (Rest_Flag == 0))
        {
          A_Flag = 0;
          B_Flag = 0;

          PJST_Timer++;

          if (Units_PJST == 2)
          {
            if (PJST_Timer / (Value_PJST * 60 * 60 * 24) % 2 == 0)
            {
              A_Flag = 1;
            }
            else
            {
              B_Flag = 1;
            }
            if (PJST_Timer / (Value_PJST * 60 * 60 * 24) >= 2)
            {
              PJST_Timer = PJST_Timer - Value_PJST * 60 * 60 * 24 * 2;
            }
          }
          else
            if (Units_PJST == 1)
            {
              if (PJST_Timer / (Value_PJST * 60 * 60) % 2 == 0)
              {
                A_Flag = 1;
              }
              else
              {
                B_Flag = 1;
              }
              if (PJST_Timer / (Value_PJST * 60 * 60) >= 2)
              {
                PJST_Timer = PJST_Timer - Value_PJST * 60 * 60 * 2;
              }
            }
            else
              if (Units_PJST == 0)
              {
                if (PJST_Timer / (Value_PJST * 60) % 2 == 0)
                {
                  A_Flag = 1;
                }
                else
                {
                  B_Flag = 1;
                }
                if (PJST_Timer / (Value_PJST * 60) >= 2)
                {
                  PJST_Timer = PJST_Timer - Value_PJST * 60 * 2;
                }
              }
        }
        else
          if (Rest_Flag != 0)
          {
            A_Flag = 0;
            B_Flag = 0;

            if (Value_PSCT > Value_Minute)
            {
              A_Flag = 1;
            }
          }

        if ((A_Flag == 1) && (B_Flag == 0))
        {
          Counter_A++;

          if (Counter_A >= 30)
          {
            DO1_Set;
            LED1_R_Off;
            LED1_G_On;
            DO2_Reset;
            LED2_R_Off;
            LED2_G_Off;
          }
        }
        else
          if ((A_Flag == 0) && (B_Flag == 1))
          {
            Counter_B++;

            if (Counter_B >= 30)
            {
              DO1_Reset;
              LED1_R_Off;
              LED1_G_Off;
              DO2_Set;
              LED2_R_Off;
              LED2_G_On;
            }
          }
          else
            if ((A_Flag == 0) && (B_Flag == 0))
            {
              DO1_Reset;
              LED1_R_Off;
              LED1_G_Off;
              DO2_Reset;
              LED2_R_Off;
              LED2_G_Off;
            }
      }
      else
        if (Mod == 1)
        {
          A_Flag = 0;
          B_Flag = 0;

          if (Pump_No == 1)
          {
            A_Flag = 1;
          }
          else
            if (Pump_No == 2)
            {
              B_Flag = 1;
            }

          if ((A_Flag == 1) && (B_Flag == 0))
          {
            Counter_A++;

            if (Counter_A >= 30)
            {
              DO1_Set;
              LED1_R_Off;
              LED1_G_On;
              DO2_Reset;
              LED2_R_Off;
              LED2_G_Off;
            }
          }
          else
            if ((A_Flag == 0) && (B_Flag == 1))
            {
              Counter_B++;

              if (Counter_B >= 30)
              {
                DO1_Reset;
                LED1_R_Off;
                LED1_G_Off;
                DO2_Set;
                LED2_R_Off;
                LED2_G_On;
              }
            }
            else
              if ((A_Flag == 0) && (B_Flag == 0))
              {
                DO1_Reset;
                LED1_R_Off;
                LED1_G_Off;
                DO2_Reset;
                LED2_R_Off;
                LED2_G_Off;
              }
        }

    if (PageNo == 1)
    {
      Loading_Page_1();
    }

  }
  RefreshDisplayPageNo = 0;
}

/*******************************************************************************
* Function Name  : EXTI3_IRQHandler
* Description    : This function handles External lines 3 interrupt request
* Input          : None
* Output         : None
* Return         : None
* Attention      : None
*******************************************************************************/
void EXTI15_10_IRQHandler(void)
{
  if (EXTI_GetITStatus(EXTI_Line13) != RESET)
  {
    EXTI_ClearITPendingBit(EXTI_Line13);

    RefreshDisplayPageNo = 1;
  }
}

