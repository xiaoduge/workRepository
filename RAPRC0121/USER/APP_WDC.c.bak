/*******************************************************************************
* Function Name  : Page_2_6
* Description    : None
* Input          : None
* Output         : None
* Return         : None
* Attention      : None
*******************************************************************************/
void Page_2_6(void)
{
  char temp_Value;

  if (PageNo == 26)
  {
    temp_Value =Read_Key();

    if (temp_Value != Key_NC)
    {
      LCM12232B_CLS();

      if (temp_Value == Key_Up)
      {
        if ((BitNo == 1) || (BitNo == 2))
        {
          Value_PJST = Value_PJST + pow(10, 2 - BitNo);
          switch (Units_PJST)
          {
            case 0://minute
              if (Value_PJST > 59)
              {
                Value_PJST = 1;
                Units_PJST++;
              }
              break;
            case 1://hour
              if (Value_PJST > 23)
              {
                Value_PJST = 1;
                Units_PJST++;
              }
              break;
            case 2://day
              if (Value_PJST > 99)
              {
                Value_PJST = 99;
              }
              break;
            default:
              break;
          }
        }
        Loading_Page_2_6();
      }
      else
        if (temp_Value == Key_Down)
        {
          if ((BitNo == 1) || (BitNo == 2))
          {
            Value_PJST = Value_PJST - pow(10, 2 - BitNo);
            switch (Units_PJST)
            {
              case 0://minute
                if (Value_PJST < 0)
                {
                  Value_PJST = 0;
                }
                break;
              case 1://hour
                if (Value_PJST < 1)
                {
                  Value_PJST = 59;
                  Units_PJST--;
                }
                break;
              case 2://day
                if (Value_PJST < 1)
                {
                  Value_PJST = 23;
                  Units_PJST--;
                }
                break;
              default:
                break;
            }
            Loading_Page_2_6();
          }
          Loading_Page_2_6();
        }
        else
          if (temp_Value == Key_Left)
          {
            if (BitNo == 1)
            {
              PageNo = 2;
              Loading_Page_2();
            }
            else
            {
              BitNo--;
              Loading_Page_2_6();
            }
          }
          else
            if (temp_Value == Key_Right)
            {
              if (BitNo == 2)
              {
                Display_Page_Saving();
                Operate_24LC01B(_24LC01_Write, Address_PJST, Value_PJST);
                Systick_Delay_N_millisecond(100);
                Operate_24LC01B(_24LC01_Write, Address_PJST + 1, Units_PJST);
                Systick_Delay_N_millisecond(100);
                PageNo = 1;
                PJST_Timer = 0;
                Loading_Page_1();
              }
              else
              {
                BitNo++;
                Loading_Page_2_6();
              }
            }
    }
  }
}

