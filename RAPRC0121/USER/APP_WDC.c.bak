* Input          : None
* Output         : None
* Return         : None
* Attention      : None
*******************************************************************************/
void Page_2_5(void)
{
  char temp_Value;

  if (PageNo == 25)
  {
    temp_Value =Read_Key();

    if (temp_Value != Key_NC)
    {
      LCM12232B_CLS();

      if (temp_Value == Key_Up)
      {
        if(Value_PSCT <=0 )
        {
          BitNo = 2;
		  Value_PSCT = Value_PSCT + pow(10,2 - BitNo);
		  if(Value_PSCT > 60)
		  { 
		    Value_PSCT = 60;
		  }
        }
			
        else if ((BitNo == 1) || (BitNo == 2))
        {
          Value_PSCT = Value_PSCT + pow(10, 2 - BitNo);
          if (Value_PSCT > 60)
          {
            Value_PSCT = 60;
          }
		
        }
		
        Loading_Page_2_5();
      }
      else
        if (temp_Value == Key_Down)
        {
          if(Value_PSCT >= 60)
          {
            BitNo = 2;
		    Value_PSCT = Value_PSCT - pow(10,2 - BitNo);
		    if(Value_PSCT < 0)
			{
			  Value_PSCT = 0;  
			}

		 }
          
			
          else if ((BitNo == 1) || (BitNo == 2))
          {
            Value_PSCT = Value_PSCT - pow(10, 2 - BitNo);
            if (Value_PSCT < 0)
            {
              Value_PSCT = 0;
            }
		 
          }
		   
          Loading_Page_2_5();
        }
        else
          if (temp_Value == Key_Left)
          {
            if (BitNo == 1)
            {
              PageNo = 2;
              Loading_Page_2();
            }
            else
            {
              BitNo--;
              Loading_Page_2_5();
            }
          }
          else
            if (temp_Value == Key_Right)
            {
              if (BitNo == 2)
              {
                Display_Page_Saving();
                Operate_24LC01B(_24LC01_Write, Address_PSCT, Value_PSCT);
                Systick_Delay_N_millisecond(100);
                PageNo = 1;
                Loading_Page_1();
              }
              else
              {
                BitNo++;
                Loading_Page_2_5();
              }
            }
    }
  }
}

