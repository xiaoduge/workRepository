        LCM12232B_Loading_Word(__6X12_, 2, 2 + 2 + 7 * 9);

        Read_GB2312_6X12_GT21L16S2W(0x5F);//'_'
        if ((BitNo == 4) || (BitNo == 5))
        {
          LCM12232B_Loading_Word(__6X12_, 2, 2 + 1 * 1 + 7 * 2 + 7 * (BitNo - 4) + 6 * 0);
