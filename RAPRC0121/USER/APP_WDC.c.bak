/*******************************************************************************
* Function Name  : Initialize_WDC
* Description    : Initialize Water Distribution Controller
* Input          : None
* Output         : None
* Return         : None
* Attention      : None
*******************************************************************************/
void Initialize_WDC(void)
{
  Mod = 0;

//  Display_Logo();
//  Systick_Delay_N_second(2);
  Operate_24LC01B(_24LC01_Read, Address_Language, 0);
  LanguageNo = RAM_24LC01B;
  if ((LanguageNo != LanguageNo_English) && (LanguageNo != LanguageNo_Simplified_Chinese))
  {
    LanguageNo = LanguageNo_Simplified_Chinese;
  }

  Operate_24LC01B(_24LC01_Read, Address_PSCT, 0);
  Value_PSCT = RAM_24LC01B;
  if ((Value_PSCT > 59) || (Value_PSCT < 1))
  {
    Value_PSCT = 3;
  }

  Operate_24LC01B(_24LC01_Read, Address_PJST, 0);
  Value_PJST = RAM_24LC01B;
  Operate_24LC01B(_24LC01_Read, Address_PJST + 1, 0);
  Units_PJST = RAM_24LC01B;
  if (Units_PJST > 2)
  {
    Units_PJST = 2;
  }
  switch (Units_PJST)
  {
    case 0://minute
      if ((Value_PJST > 59) || (Value_PJST < 1))
      {
        Value_PJST = 3;
      }
      break;
    case 1://hour
      if ((Value_PJST > 23) || (Value_PJST < 1))
      {
        Value_PJST = 1;
      }
      break;
    case 2://day
      if ((Value_PJST > 99) || (Value_PJST < 1))
      {
        Value_PJST = 1;
      }
    default:
    break;
  }

  Operate_24LC01B(_24LC01_Read, Address_Pump_Select, 0);
  Pump_No = RAM_24LC01B;
  if ((Pump_No < 1) || (Pump_No > 2))
  {
    Pump_No = 1;
  }

  PageNo = 1;
  Page_2No = 1;
  
  Display_Logo();     //logo
  Systick_Delay_N_second(2);

  Display_Product_Title();
  Systick_Delay_N_second(2);
  Operate_24LC01B(_24LC01_Read, Address_Start, 0);
  Start_Counter = RAM_24LC01B;
  if (Start_Counter != 1)
  {
    for (Weekday_No = 0; Weekday_No < 7; Weekday_No++)
    {
      Operate_24LC01B(_24LC01_Write, Address_Weekday + 4 * (Weekday_No), 0x00);
      Systick_Delay_N_millisecond(100);
    }
    for (Holiday_No = 1; Holiday_No <= 70; Holiday_No++)
    {
      Operate_24LC01B(_24LC01_Write, Address_Holiday + 3 * (Holiday_No - 1) + 1, 0x00);
      Systick_Delay_N_millisecond(100);
    }
    Operate_24LC01B(_24LC01_Write, Address_Start, 1);
    Systick_Delay_N_millisecond(100);
  }
  for (Weekday_No = 0; Weekday_No < 7; Weekday_No++)
  {
    Operate_24LC01B(_24LC01_Read, Address_Weekday + 4 * Weekday_No + 0, 0);
    Weekday_Data[Weekday_No][Weekday_Hour_Start] = RAM_24LC01B;
    Operate_24LC01B(_24LC01_Read, Address_Weekday + 4 * Weekday_No + 1, 0);
    Weekday_Data[Weekday_No][Weekday_Minute_Start] = RAM_24LC01B;
    Operate_24LC01B(_24LC01_Read, Address_Weekday + 4 * Weekday_No + 2, 0);
    Weekday_Data[Weekday_No][Weekday_Hour_Stop] = RAM_24LC01B;
    Operate_24LC01B(_24LC01_Read, Address_Weekday + 4 * Weekday_No + 3, 0);
    Weekday_Data[Weekday_No][Weekday_Minute_Stop] = RAM_24LC01B;
  }
  for (Holiday_No = 1; Holiday_No <= 70; Holiday_No++)
  {
    Operate_24LC01B(_24LC01_Read, Address_Holiday + 3 * (Holiday_No - 1) + 0, 0);
    Holiday_Data[Holiday_No - 1][Holiday_Year] = RAM_24LC01B;
    Operate_24LC01B(_24LC01_Read, Address_Holiday + 3 * (Holiday_No - 1) + 1, 0);
    Holiday_Data[Holiday_No - 1][Holiday_Month] = RAM_24LC01B;
    Operate_24LC01B(_24LC01_Read, Address_Holiday + 3 * (Holiday_No - 1) + 2, 0);
    Holiday_Data[Holiday_No - 1][Holiday_Day] = RAM_24LC01B;
  }

//  Display_Revision();
//  Systick_Delay_N_second(2);

  Operate_PCF8563(PCF8563_Read, PCF8563_Register_Hours, 0);//Read Hours
  Value_Hour = ((RAM_PCF8563 >> 4) & 0x03) * 10 + (RAM_PCF8563 & 0x0F);
  Operate_PCF8563(PCF8563_Read, PCF8563_Register_Minutes, 0);//Read Minutes
  Value_Minute = ((RAM_PCF8563 >> 4) & 0x07) * 10 + (RAM_PCF8563 & 0x0F);
  Operate_PCF8563(PCF8563_Read, PCF8563_Register_VL_Seconds, 0);//Read Seconds
  Value_Second = ((RAM_PCF8563 >> 4) & 0x07) * 10 + (RAM_PCF8563 & 0x0F);
  Operate_PCF8563(PCF8563_Read, PCF8563_Register_Years, 0);//Read Year
  Value_Year = ((RAM_PCF8563 >> 4) & 0x07) * 10 + (RAM_PCF8563 & 0x0F);
  Operate_PCF8563(PCF8563_Read, PCF8563_Register_Century_Months, 0);//Read Month
  Value_Month = ((RAM_PCF8563 >> 4) & 0x01) * 10 + (RAM_PCF8563 & 0x0F);
  Operate_PCF8563(PCF8563_Read, PCF8563_Register_Days, 0);//Read Day
  Value_Day = ((RAM_PCF8563 >> 4) & 0x03) * 10 + (RAM_PCF8563 & 0x0F);
  Operate_PCF8563(PCF8563_Read, PCF8563_Register_Weekdays, 0);//Read Weekdays
  Value_Weekday = RAM_PCF8563 & 0x07;

  Loading_Page_1();

  Start_Flag = 1;


  Counter_A = 30;
  Counter_B = 30;
}

