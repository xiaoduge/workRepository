* Input          : None
* Output         : None
* Return         : None
* Attention      : None
*******************************************************************************/
void Page_2(void)
{
  char temp_Value;

  if (PageNo == 2)
  {
    temp_Value = Read_Key();

    if (temp_Value != Key_NC)
    {
      LCM12232B_CLS();

      if (temp_Value == Key_Up)
      {
        if (Page_2No == Page_2No_Min)
        {
          Page_2No = Page_2No_Max;
        }
        else
        {
          Page_2No--;
        }
        Loading_Page_2();
      }
      else
        if (temp_Value == Key_Down)
        {
          if (Page_2No == Page_2No_Max)
          {
            Page_2No = Page_2No_Min;
          }
          else
          {
            Page_2No++;
          }
          Loading_Page_2();
        }
        else
          if (temp_Value == Key_Left)
          {
            PageNo = 1;
            Loading_Page_1();
          }
          else
            if (temp_Value == Key_Right)
            {
              switch (Page_2No)
              {
                case 1:
                  Loading_Page_2_1();
                  PageNo = 21;
                  break;
                case 2:
                  BitNo = 1;
                  Loading_Page_2_2();
                  PageNo = 22;
                  break;
                case 3:
				
                  BitNo = 1;
                  Menu_Operation = 1;
                  Weekday_No = 0; 
                  Weekday_Function = Weekday_Data[Weekday_No][Weekday_Hour_Start]>> 7 & 0x01;//weekday &0x01
                  Loading_Page_2_3();
                  PageNo = 23;
                  break;
                case 4:
                  BitNo = 1;
                  Menu_Operation = 1;
                  Holiday_No = 1;
                  Holiday_Function = (Holiday_Data[0][Holiday_Month] >> 4) & 0x01;
                  Holiday_Data[0][Holiday_Month] &= 0x0F;
                  Loading_Page_2_4();
                  PageNo = 24;
                  break;
                case 5:
                  BitNo = 1;
                  Loading_Page_2_5();
                  PageNo = 25;
                  break;
                case 6:
                  BitNo = 1;
                  Loading_Page_2_6();
                  PageNo = 26;
                  break;
                case 7:
                  Loading_Page_2_7();
                  PageNo = 27;
                  break;
                default:
                  break;
              }
            }
    }
  }
}

/*******************************************************************************
* Function Name  : Loading_Page_2
* Description    : None
* Input          : None
* Output         : None
* Return         : None
* Attention      : None
*******************************************************************************/
void Loading_Page_2(void)
{
  unsigned char i, j;

  for (i = 0; i < 4; i++)
  {
    for (j = 0; j < 122; j++)
    {
      Ram_LM12232B[(i * 122) + j] = ArrowUp_and_ArrowDown[((3 - i) * 122) + j];
    }
  }
  for (i = 0; i < 2; i++)
  {
    for (j = 0; j < 122; j++)
    {
      Ram_LM12232B[(i * 122) + j] |= Page_2_Line1[LanguageNo - 1][((1 - i) * 122) + j];
    }
  }
  for (i = 2; i < 4; i++)
  {
    for (j = 0; j < 122; j++)
    {
      Ram_LM12232B[(i * 122) + j] |= Page_2_Line2[LanguageNo - 1][Page_2No - 1][((1 - (i - 2)) * 122) + j];
    }
  }
  LCM12232B_Display_Page();
}

