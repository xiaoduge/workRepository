/*******************************************************************************
* Function Name  : Page_2_2
* Description    : None
* Input          : None
* Output         : None
* Return         : None
* Attention      : None
*******************************************************************************/
void Page_2_2(void)
{
  char temp_Value;

  if (PageNo == 22)
  {
    temp_Value = Read_Key();

    if (temp_Value != Key_NC)
    {
      LCM12232B_CLS();

      if (temp_Value == Key_Up)
      {
        if ((BitNo == 1) || (BitNo == 2))
        {
          Value_Year = Value_Year + pow(10, 2 - BitNo);
          if (Value_Year > Value_Year_Max)
          {
            Value_Year = Value_Year_Max;
          }
        }
        else
          if ((BitNo == 3) || (BitNo == 4))
          {
            Value_Month = Value_Month + pow(10, 4 - BitNo);
            if (Value_Month > Value_Month_Max)
            {
              Value_Month = Value_Month_Max;
            }
          }
          else
            if ((BitNo == 5) || (BitNo == 6))
            {
              Value_Day = Value_Day + pow(10, 6 - BitNo);
              if (Value_Day > Value_Day_Max)
              {
                Value_Day = Value_Day_Max;
              }
            }
            else
              if ((BitNo == 7) || (BitNo == 8))
              {
                Value_Hour = Value_Hour + pow(10, 8 - BitNo);
                if (Value_Hour > Value_Hour_Max)
                {
                  Value_Hour = Value_Hour_Max;
                }
              }
              else
                if ((BitNo == 9) || (BitNo == 10))
                {
                  Value_Minute = Value_Minute + pow(10, 10 - BitNo);
                  if (Value_Minute > Value_Minute_Max)
                  {
                    Value_Minute = Value_Minute_Max;
                  }
                }
                else
                  if (BitNo == 11)
                  {
                    Value_Weekday = Value_Weekday + 1;
                    if (Value_Weekday > 6)
                    {
                      Value_Weekday = 0;
                    }
                  }
        Loading_Page_2_2();
      }
      else
        if (temp_Value == Key_Down)
        {
          if ((BitNo == 1) || (BitNo == 2))
          {
            Value_Year = Value_Year - pow(10, 2 - BitNo);
            if (Value_Year < Value_Year_Min)
            {
              Value_Year = Value_Year_Min;
            }
          }
          else
            if ((BitNo == 3) || (BitNo == 4))
            {
              Value_Month = Value_Month - pow(10, 4 - BitNo);
              if (Value_Month < Value_Month_Min)
              {
                Value_Month = Value_Month_Min;
              }
            }
            else
              if ((BitNo == 5) || (BitNo == 6))
              {
                Value_Day = Value_Day - pow(10, 6 - BitNo);
                if (Value_Day < Value_Day_Min)
                {
                  Value_Day = Value_Day_Min;
                }
              }
              else
                if ((BitNo == 7) || (BitNo == 8))
                {
                  Value_Hour = Value_Hour - pow(10, 8 - BitNo);
                  if (Value_Hour < Value_Hour_Min)
                  {
                    Value_Hour = Value_Hour_Min;
                  }
                }
                else
                  if ((BitNo == 9) || (BitNo == 10))
                  {
                    Value_Minute = Value_Minute - pow(10, 10 - BitNo);
                    if (Value_Minute < Value_Minute_Min)
                    {
                      Value_Minute = Value_Minute_Min;
                    }
                  }
                  else
                    if (BitNo == 11)
                    {
                      Value_Weekday = Value_Weekday -1;
                      if (Value_Weekday < 0)
                      {
                        Value_Weekday = 6;
                      }
                    }
          Loading_Page_2_2();
        }
        else
          if (temp_Value == Key_Left)
          {
            if (BitNo == 1)
            {
              PageNo = 2;
              Loading_Page_2();
            }
            else
            {
              BitNo--;
              Loading_Page_2_2();
            }
          }
          else
            if (temp_Value == Key_Right)
            {
              if (BitNo == 11)
              {
                Display_Page_Saving();
                Operate_PCF8563(PCF8563_Write, PCF8563_Register_Hours, ((Value_Hour / 10) << 4) | (Value_Hour % 10));//Write Hours
                Systick_Delay_N_millisecond(100);
                Operate_PCF8563(PCF8563_Write, PCF8563_Register_Minutes, ((Value_Minute / 10) << 4) | (Value_Minute % 10));//Write Minutes
                Systick_Delay_N_millisecond(100);
                Value_Second = 0;
                Operate_PCF8563(PCF8563_Write, PCF8563_Register_VL_Seconds, ((Value_Second / 10) << 4) | (Value_Second % 10));//Write Seconds
                Systick_Delay_N_millisecond(100);
                Operate_PCF8563(PCF8563_Write, PCF8563_Register_Years, ((Value_Year / 10) << 4) | (Value_Year % 10));//Write Year
                Systick_Delay_N_millisecond(100);
                Operate_PCF8563(PCF8563_Write, PCF8563_Register_Century_Months, ((Value_Month / 10) << 4) | (Value_Month % 10));//Write Month
                Systick_Delay_N_millisecond(100);
                Operate_PCF8563(PCF8563_Write, PCF8563_Register_Days, ((Value_Day / 10) << 4) | (Value_Day % 10));//Write Day
                Systick_Delay_N_millisecond(100);
                Operate_PCF8563(PCF8563_Write, PCF8563_Register_Weekdays, Value_Weekday);//Write Weekday
                Systick_Delay_N_millisecond(100);
                PageNo = 1;
                Loading_Page_1();
              }
              else
              {
                BitNo++;
                Loading_Page_2_2();
              }
            }
    }
  }
}

/*******************************************************************************
* Function Name  : Loading_Page_2_2
* Description    : None
* Input          : None
* Output         : None
* Return         : None
* Attention      : None
*******************************************************************************/
void Loading_Page_2_2(void)
{
  unsigned char i, j;

  switch (LanguageNo - 1)
  {
    case 0:
      /* Date */
      /* Display Year*/
      Read_GB2312_6X12_GT21L16S2W(0x30 + 2);//2
      LCM12232B_Loading_Word(__6X12_, 1, 2 + 2 + 7 * 0);
      Read_GB2312_6X12_GT21L16S2W(0x30 + 0);//0
      LCM12232B_Loading_Word(__6X12_, 1, 2 + 2 + 7 * 1);
      Read_GB2312_6X12_GT21L16S2W(0x30 + (Value_Year % 100) / 10);
      LCM12232B_Loading_Word(__6X12_, 1, 2 + 2 + 7 * 2);
      Read_GB2312_6X12_GT21L16S2W(0x30 + (Value_Year % 10) / 1);
      LCM12232B_Loading_Word(__6X12_, 1, 2 + 2 + 7 * 3);

      Read_GB2312_6X12_GT21L16S2W(0x2D);//'-'
      LCM12232B_Loading_Word(__6X12_, 1, 2 + 2 + 7 * 4 - 1);
      /* Display Month*/
      Read_GB2312_6X12_GT21L16S2W(0x30 + (Value_Month % 100) / 10);
      LCM12232B_Loading_Word(__6X12_, 1, 2 + 2 + 7 * 5);
      Read_GB2312_6X12_GT21L16S2W(0x30 + (Value_Month % 10) / 1);
      LCM12232B_Loading_Word(__6X12_, 1, 2 + 2 + 7 * 6);

      Read_GB2312_6X12_GT21L16S2W(0x2D);//'-'
      LCM12232B_Loading_Word(__6X12_, 1, 2 + 2 + 7 * 7 - 1);
      /* Display Day*/
      Read_GB2312_6X12_GT21L16S2W(0x30 + (Value_Day % 100) / 10);
      LCM12232B_Loading_Word(__6X12_, 1, 2 + 2 + 7 * 8);
      Read_GB2312_6X12_GT21L16S2W(0x30 + (Value_Day % 10) / 1);
      LCM12232B_Loading_Word(__6X12_, 1, 2 + 2 + 7 * 9);
      /* Time */
      /* Display Hour*/
      Read_GB2312_6X12_GT21L16S2W(0x30 + (Value_Hour % 100) / 10);
      LCM12232B_Loading_Word(__6X12_, 2, 2+ 2 + 7 * 0);
      Read_GB2312_6X12_GT21L16S2W(0x30 + (Value_Hour % 10) / 1);
      LCM12232B_Loading_Word(__6X12_, 2, 2+ 2 + 7 * 1);

      Read_GB2312_6X12_GT21L16S2W(0x3A);//':'
      LCM12232B_Loading_Word(__6X12_, 2, 2+ 2 + 7 * 2 + 6 * 0 - 1);
      /* Display Minute */
      Read_GB2312_6X12_GT21L16S2W(0x30 + (Value_Minute % 100) / 10);
      LCM12232B_Loading_Word(__6X12_, 2, 2+ 2 + 7 * 2 + 6 * 1);
      Read_GB2312_6X12_GT21L16S2W(0x30 + (Value_Minute % 10) / 1);
      LCM12232B_Loading_Word(__6X12_, 2, 2+ 2 + 7 * 3 + 6 * 1);

      Read_GB2312_6X12_GT21L16S2W(0x5F);//'_'
      if ((BitNo == 1) || (BitNo == 2))
      {
        LCM12232B_Loading_Word(__6X12_, 1, 2 + 1 * 1 + 7 * 2 + 7 * (BitNo - 1) + 6 * 0);
      }
      else
        if ((BitNo == 3) || (BitNo == 4))
        {
          LCM12232B_Loading_Word(__6X12_, 1, 2 + 1 * 2 + 7 * 2 + 7 * (BitNo - 1) + 6 * 1);
        }
        else
          if ((BitNo == 5) || (BitNo == 6))
          {
            LCM12232B_Loading_Word(__6X12_, 1, 2 + 1 * 3 + 7 * 2 + 7 * (BitNo - 1) + 6 * 2);
          }
          else
            if ((BitNo == 7) || (BitNo == 8))
            {
              LCM12232B_Loading_Word(__6X12_, 2, 2 + 1 + 7 * (BitNo - 7) + 6 * 0);
            }
            else
              if ((BitNo == 9) || (BitNo == 10))
              {
                LCM12232B_Loading_Word(__6X12_, 2, 2 + 1 + 7 * (BitNo - 7) + 6 * 1);
              }
      break;
    case 1:
      /* Date */
      /* Display Year*/
      Read_GB2312_11X12_GT21L16S2W(0xA3B0 + 2);//2
      LCM12232B_Loading_Word(__11X12_, 1, 2 + 9 * 0);
      Read_GB2312_11X12_GT21L16S2W(0xA3B0 + 0);//0
      LCM12232B_Loading_Word(__11X12_, 1, 2 + 9 * 1);
      Read_GB2312_11X12_GT21L16S2W(0xA3B0 + (Value_Year % 100) / 10);
      LCM12232B_Loading_Word(__11X12_, 1, 2 + 9 * 2);
      Read_GB2312_11X12_GT21L16S2W(0xA3B0 + (Value_Year % 10) / 1);
      LCM12232B_Loading_Word(__11X12_, 1, 2 + 9 * 3);

      Read_GB2312_11X12_GT21L16S2W(0xA3AD);//'-'
      LCM12232B_Loading_Word(__11X12_, 1, 2 + 9 * 4);
      /* Display Month*/
      Read_GB2312_11X12_GT21L16S2W(0xA3B0 + (Value_Month % 100) / 10);
      LCM12232B_Loading_Word(__11X12_, 1, 2 + 9 * 5);
      Read_GB2312_11X12_GT21L16S2W(0xA3B0 + (Value_Month % 10) / 1);
      LCM12232B_Loading_Word(__11X12_, 1, 2 + 9 * 6);

      Read_GB2312_11X12_GT21L16S2W(0xA3AD);//'-'
      LCM12232B_Loading_Word(__11X12_, 1, 2 + 9 * 7);
      /* Display Day*/
      Read_GB2312_11X12_GT21L16S2W(0xA3B0 + (Value_Day % 100) / 10);
      LCM12232B_Loading_Word(__11X12_, 1, 2 + 9 * 8);
      Read_GB2312_11X12_GT21L16S2W(0xA3B0 + (Value_Day % 10) / 1);
      LCM12232B_Loading_Word(__11X12_, 1, 2 + 9 * 9);
      /* Time */
      /* Display Hour*/
      Read_GB2312_11X12_GT21L16S2W(0xA3B0 + (Value_Hour % 100) / 10);
      LCM12232B_Loading_Word(__11X12_, 2, 2 + 9 * 0);
      Read_GB2312_11X12_GT21L16S2W(0xA3B0 + (Value_Hour % 10) / 1);
      LCM12232B_Loading_Word(__11X12_, 2, 2 + 9 * 1);

      Read_GB2312_11X12_GT21L16S2W(0xA1C3);//':'
      LCM12232B_Loading_Word(__11X12_, 2, 2 + 9 * 2 + 6 * 0);
      /* Display Minute */
      Read_GB2312_11X12_GT21L16S2W(0xA3B0 + (Value_Minute % 100) / 10);
      LCM12232B_Loading_Word(__11X12_, 2, 2 + 9 * 2 + 6 * 1);
      Read_GB2312_11X12_GT21L16S2W(0xA3B0 + (Value_Minute % 10) / 1);
      LCM12232B_Loading_Word(__11X12_, 2, 2 + 9 * 3 + 6 * 1);

      Read_GB2312_11X12_GT21L16S2W(0xA3DF);//'_'
      if ((BitNo == 1) || (BitNo == 2))
      {
        LCM12232B_Loading_Word(__11X12_, 1, 2 + 9 * 2 + 9 * (BitNo - 1) + 9 * 0);
      }
      else
        if ((BitNo == 3) || (BitNo == 4))
        {
          LCM12232B_Loading_Word(__11X12_, 1, 2 + 9 * 2 + 9 * (BitNo - 1) + 9 * 1);
        }
        else
          if ((BitNo == 5) || (BitNo == 6))
          {
            LCM12232B_Loading_Word(__11X12_, 1, 2 + 9 * 2 + 9 * (BitNo - 1) + 9 * 2);
          }
          else
            if ((BitNo == 7) || (BitNo == 8))
            {
              LCM12232B_Loading_Word(__11X12_, 2, 2 + 9 * (BitNo - 7) + 6 * 0);
            }
            else
              if ((BitNo == 9) || (BitNo == 10))
              {
                LCM12232B_Loading_Word(__11X12_, 2, 2 + 9 * (BitNo - 7) + 6 * 1);
              }
      break;
  }
  if (BitNo == 11)
  {
    for (i = 2; i < 4; i++)
    {
      for (j = 0; j < 122; j++)
      {
        Ram_LM12232B[(i * 122) + j] |= Page_2_2_Line2[LanguageNo - 1][Value_Weekday][((1 - (i - 2)) * 122) + j];
      }
    }
  }
  else
  {
    for (i = 2; i < 4; i++)
    {
      for (j = 0; j < 122; j++)
      {
        Ram_LM12232B[(i * 122) + j] |= Page_1_Line2[LanguageNo - 1][Value_Weekday][((1 - (i - 2)) * 122) + j];
      }
    }
  }

  LCM12232B_Display_Page();
}

/*******************************************************************************
* Function Name  : Page_2_3
* Description    : None
* Input          : None
* Output         : None
* Return         : None
* Attention      : None
*******************************************************************************/
void Page_2_3(void)
{
  unsigned char i;
  char temp_Value;

  if (PageNo == 23)
  {
    temp_Value = Read_Key();

    if (temp_Value != Key_NC)
    {
      LCM12232B_CLS();

      if (temp_Value == Key_Up)
      {
        if (BitNo == 1)
        {
          Weekday_Data[Weekday_No][Weekday_Hour_Start] = (Weekday_Data[Weekday_No][Weekday_Hour_Start] & 0x7F) | (Weekday_Function << 7);
          Weekday_No++;
          if (Weekday_No > 6)
          {
            Weekday_No = 0;
          }
          Weekday_Function = (Weekday_Data[Weekday_No][Weekday_Hour_Start] >> 7) & 0x01;
        }
        else
          if (BitNo == 2)
          {
            Weekday_Function++;
            if (Weekday_Function > 1)
            {
              Weekday_Function = 0;
            }
          }
          else
            if ((BitNo == 3) || (BitNo == 4))
            {
              Weekday_Data[Weekday_No][Weekday_Hour_Start] = (Weekday_Data[Weekday_No][Weekday_Hour_Start] & 0x7F) + pow(10, 4 - BitNo);
              if (Weekday_Data[Weekday_No][Weekday_Hour_Start] > 23)
              {
                Weekday_Data[Weekday_No][Weekday_Hour_Start] = 23;
              }
            }
            else
              if ((BitNo == 5) || (BitNo == 6))
              {
                Weekday_Data[Weekday_No][Weekday_Minute_Start] = Weekday_Data[Weekday_No][Weekday_Minute_Start] + pow(10, 6 - BitNo);
                if (Weekday_Data[Weekday_No][Weekday_Minute_Start] > 59)
                {
                  Weekday_Data[Weekday_No][Weekday_Minute_Start] = 59;
                }
              }
              else
                if ((BitNo == 7) || (BitNo == 8))
                {
                  Weekday_Data[Weekday_No][Weekday_Hour_Stop] = Weekday_Data[Weekday_No][Weekday_Hour_Stop] + pow(10, 8 - BitNo);
                  if (Weekday_Data[Weekday_No][Weekday_Hour_Stop] > 23)
                  {
                    Weekday_Data[Weekday_No][Weekday_Hour_Stop] = 23;
                  }
                }
                else
                  if ((BitNo == 9) || (BitNo == 10))
                  {
                    Weekday_Data[Weekday_No][Weekday_Minute_Stop] = Weekday_Data[Weekday_No][Weekday_Minute_Stop] + pow(10, 10 - BitNo);
                    if (Weekday_Data[Weekday_No][Weekday_Minute_Stop] > 59)
                    {
                      Weekday_Data[Weekday_No][Weekday_Minute_Stop] = 59;
                    }
                  }
                  else
                    if (BitNo == 11)
                    {
                      Menu_Operation++;
                      if (Menu_Operation > 2)
                      {
                        Menu_Operation = 0;
                      }
                    }
        Loading_Page_2_3();
      }
      else
        if (temp_Value == Key_Down)
        {
          if (BitNo == 1)
          {
            Weekday_Data[Weekday_No][Weekday_Hour_Start] = (Weekday_Data[Weekday_No][Weekday_Hour_Start] & 0x7F) | (Weekday_Function << 7);
            Weekday_No--;
            if (Weekday_No < 0)
            {
              Weekday_No = 6;
            }
	            Weekday_Function = (Weekday_Data[Weekday_No][Weekday_Hour_Start] >> 7) & 0x01;
          }
          else
            if (BitNo == 2)
            {
              Weekday_Function--;
              if (Weekday_Function < 0)
              {
                Weekday_Function = 1;
              }
            }
            else
              if ((BitNo == 3) || (BitNo == 4))
              {
                Weekday_Data[Weekday_No][Weekday_Hour_Start] = (Weekday_Data[Weekday_No][Weekday_Hour_Start] & 0x7F) - pow(10, 4 - BitNo);
                if (Weekday_Data[Weekday_No][Weekday_Hour_Start] < 0)
                {
